{{#each this}}
<li class="cards__item hvr-buzz-out" tabindex="0" data-eventId={{id}}>
  <div class="thumb">
    <img class="cards__img" src={{images.5.url}} alt={{name}} width="180" height="227" />
  </div>  
  <h3 class="item__title">{{name}}</h3>
  <p class="item__date">{{dates.start.localDate}}</p>
  <p class="item__location">
    {{dates.timezone}}
  </p>
</li>
{{/each}}